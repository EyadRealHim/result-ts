  "description": "Tiny Result is a simple yet powerful implementation of the Result Type. It provides a Result type that is used for returning and propagating errors in a convenient and efficient manner.",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:release": "tsup ./src/index.ts --format cjs,esm --dts",
    "build:lint": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EyadRealHim/tiny-result.git"
  },
  "author": "EyadRealHim",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EyadRealHim/tiny-result/issues"
  },
  "homepage": "https://github.com/EyadRealHim/tiny-result#readme",
  "devDependencies": {
    "typescript": "^5.1.6",
    "tsup": "^7.1.0"
  },
  "keywords": [
    "tiny-result",
    "result",
    "error-handling",
    "error-management"
  ]
}
